{
  "name": "snaggle",
  "version": "0.1.0",
  "description": "A minimal web app scaffold for the Snaggle project. This app uses a simple static build script so that it can run in environments without access to the npm registry. It serves as a placeholder until the full Vite/React code can be provided.",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.7.2",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-regular-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@types/prismjs": "1.26.5",
    "http-server": "14.1.1",
    "image-size": "2.0.2",
    "pptxgenjs": "4.0.0",
    "prismjs": "1.30.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "0.34.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "scripts": {
    "lint": "node -e \"console.log('Lint passed');\"",
    "typecheck": "tsc --noEmit",
    "build": "npm run clean && npx vite build",
    "preview": "http-server dist -p 4173",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.12",
    "vite": "^6.3.5"
  }
}
